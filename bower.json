{
    "name": "express-angular-starter",
    "description": "A simple login demo using Express.js and Angular",
    "main": "index.js",
    "authors": [
        "Vijay Kandy"
    ],
    "license": "ISC",
    "keywords": [
        "login"
    ],
    "homepage": "https://github.com/vkandy/express-angular-starter",
    "moduleType": [
        "node"
    ],
    "private": true,
    "ignore": [
        "**/.*",
        "node_modules",
        "bower_components",
        "client/vendor",
        "test",
        "tests"
    ],
    "dependencies": {
        "angular": "^1.5.3",
        "angular-bootstrap": "^1.3.1",
        "angular-jwt": "^0.0.9",
        "angular-messages": "^1.5.3",
        "angular-ui-router": "^0.2.18",
        "angular-storage": "^0.5.0",
        "angular-resource": "^1.5.3",
        "bootstrap": "^3.3.6",
        "font-awesome": "^4.6.0",
        "jquery": "^2.2.3"
    },
    "overrides": {
        "bootstrap": {
            "main": [
                "dist/css/bootstrap.css",
                "dist/fonts/*",
                "dist/js/bootstrap.js"
            ]
        },
        "font-awesome": {
            "main": [
                "fonts/*",
                "css/font-awesome.css"
            ]
        }
    },
    "resolutions": {
        "angular": "1.5.3"
    }
}
